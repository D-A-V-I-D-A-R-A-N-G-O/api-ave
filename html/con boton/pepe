let caRita = () => {
                let pepe
                if (datos.Viene === "Mucho") { 
                    pepe = [datos.Viene, "😁"];
                } else if (datos.Viene === "A Veces") { 
                    pepe = [datos.Viene, "😐"];
                } else if (datos.Viene === "Casi Siempre") {
                    pepe = [datos.Viene,  "😅"];
                } else if  (datos.Viene === "Ya No") {
                    pepe = [datos.Viene, "😔"];
                } else if(datos.Viene)
                    pepe = [datos.Viene]
                return pepe
            }